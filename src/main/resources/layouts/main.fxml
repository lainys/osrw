<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" minWidth="500" minHeight="400" alignment="center" hgap="10" vgap="10"
          xmlns="http://javafx.com/javafx/8.0.121" fx:controller="ru.miet.orgact.controllers.MainController">
    <children>
        <TabPane prefWidth="1000" prefHeight="800" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="Untitled Tab 1">
                    <fx:include fx:id="firstTabPage" source="first_tab.fxml"/>
                </Tab>
                <Tab text="Untitled Tab 2">
                    <fx:include fx:id="secondTabPage" source="second_tab.fxml"/>
                </Tab>
                <Tab text="Untitled Tab">
                    <fx:include fx:id="thirdTabPage" source="third_tab.fxml"/>
                </Tab>
            </tabs>
        </TabPane>
    </children>
    <columnConstraints>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
</GridPane>
