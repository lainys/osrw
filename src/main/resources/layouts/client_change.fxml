<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" fx:id="settingGrid" maxHeight="-Infinity" maxWidth="-Infinity"
          prefHeight="227.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121"
          fx:controller="ru.miet.orgact.controllers.ClientSetting">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="72.0" minHeight="10.0" prefHeight="59.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="80.0" minHeight="10.0" prefHeight="63.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="69.0" minHeight="10.0" prefHeight="65.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label text="ip адрес">
            <font>
                <Font size="14.0"/>
            </font>
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
        </Label>
        <TextField fx:id="ip" prefHeight="25.0" prefWidth="140.0" promptText="Введите ip" text="10.32.0.1"
                   GridPane.columnIndex="1">
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
        </TextField>
        <Label text="Порт" GridPane.rowIndex="1">
            <font>
                <Font size="14.0"/>
            </font>
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
        </Label>
        <TextField fx:id="port" prefHeight="25.0" prefWidth="140.0" promptText="Введите порт" text="9000"
                   GridPane.columnIndex="1" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
        </TextField>
        <Button mnemonicParsing="false" onAction="#change" prefHeight="41.0" prefWidth="134.0" text="Изменить"
                GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="2">
            <font>
                <Font size="15.0"/>
            </font>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
        </Button>
        <Button mnemonicParsing="false" onAction="#reset" prefHeight="41.0" prefWidth="134.0" text="Сбросить"
                GridPane.rowIndex="2">
            <font>
                <Font size="15.0"/>
            </font>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
        </Button>
    </children>
</GridPane>
