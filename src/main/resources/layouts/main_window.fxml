<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" fx:id="startWindow" prefHeight="222.0" prefWidth="420.0"
          xmlns="http://javafx.com/javafx/8.0.121" fx:controller="ru.miet.orgact.controllers.StartWindow">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#buttonAddClick"
                prefHeight="45.0" prefWidth="170.0" text="Добавить публикацию" GridPane.halignment="RIGHT">
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
        </Button>
        <Button contentDisplay="CENTER" graphicTextGap="1.0" mnemonicParsing="false" onAction="#buttonEditClick"
                prefHeight="43.0" text="Редактировать публикацию" GridPane.columnIndex="1" GridPane.halignment="LEFT">
            <GridPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </GridPane.margin>
        </Button>
    </children>
</GridPane>
