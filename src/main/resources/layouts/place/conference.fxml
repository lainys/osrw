<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="414.0" prefWidth="468.0"
          xmlns="http://javafx.com/javafx/8.0.121" fx:controller="ru.miet.orgact.controllers.ConferenceController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="308.0" minWidth="10.0" prefWidth="222.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="306.0" minWidth="0.0" prefWidth="235.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="39.0" minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="45.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="43.0" minHeight="10.0" prefHeight="37.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="80.0" minHeight="0.0" prefHeight="35.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="96.0" minHeight="0.0" prefHeight="32.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="131.0" minHeight="0.0" prefHeight="40.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="131.0" minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="131.0" minHeight="10.0" prefHeight="37.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label text="Название конференции" GridPane.rowIndex="1">
            <font>
                <Font size="14.0"/>
            </font>
            <GridPane.margin>
                <Insets left="10.0" right="10.0"/>
            </GridPane.margin>
        </Label>
        <TextField fx:id="conferenceName" prefHeight="25.0" prefWidth="140.0" promptText="Введите название конференции"
                   GridPane.columnIndex="1" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="10.0" right="10.0"/>
            </GridPane.margin>
        </TextField>
        <Label text="Дата начала конференции" GridPane.rowIndex="2">
            <font>
                <Font size="14.0"/>
            </font>
            <GridPane.margin>
                <Insets left="10.0" right="10.0"/>
            </GridPane.margin>
        </Label>
        <DatePicker fx:id="conferenceStart" prefHeight="25.0" promptText="Дата начала" GridPane.columnIndex="1"
                    GridPane.rowIndex="2">
            <GridPane.margin>
                <Insets left="10.0" right="10.0"/>
            </GridPane.margin>
        </DatePicker>
        <Label text="Дата окончания конференции" GridPane.rowIndex="3">
            <font>
                <Font size="14.0"/>
            </font>
            <GridPane.margin>
                <Insets left="10.0" right="10.0"/>
            </GridPane.margin>
        </Label>
        <DatePicker fx:id="conferenceFinish" prefHeight="25.0" promptText="Дата окончания" showWeekNumbers="true"
                    GridPane.columnIndex="1" GridPane.rowIndex="3">
            <GridPane.margin>
                <Insets left="10.0" right="10.0"/>
            </GridPane.margin>
        </DatePicker>
        <Label text="Страна проведения" GridPane.rowIndex="4">
            <font>
                <Font size="14.0"/>
            </font>
            <GridPane.margin>
                <Insets left="10.0" right="10.0"/>
            </GridPane.margin>
        </Label>
        <TextField fx:id="conferenceCountry" prefHeight="25.0" prefWidth="140.0" promptText="Введите страну проведения"
                   GridPane.columnIndex="1" GridPane.rowIndex="4">
            <GridPane.margin>
                <Insets left="10.0" right="10.0"/>
            </GridPane.margin>
        </TextField>
        <Label text="Город проведения" GridPane.rowIndex="5">
            <font>
                <Font size="14.0"/>
            </font>
            <GridPane.margin>
                <Insets left="10.0" right="10.0"/>
            </GridPane.margin>
        </Label>
        <TextField fx:id="conferenceCity" prefHeight="25.0" prefWidth="140.0" promptText="Введите город проведения"
                   GridPane.columnIndex="1" GridPane.rowIndex="5">
            <GridPane.margin>
                <Insets left="10.0" right="10.0"/>
            </GridPane.margin>
        </TextField>
        <Label text="Интернет ссылка" GridPane.rowIndex="7">
            <font>
                <Font size="14.0"/>
            </font>
            <GridPane.margin>
                <Insets left="10.0" right="10.0"/>
            </GridPane.margin>
        </Label>
        <TextField fx:id="conferenceLink" prefHeight="25.0" prefWidth="140.0" promptText="Вставьте ссылку"
                   GridPane.columnIndex="1" GridPane.rowIndex="7">
            <GridPane.margin>
                <Insets left="10.0" right="10.0"/>
            </GridPane.margin>
        </TextField>
        <Label text="Страницы" GridPane.rowIndex="6">
            <font>
                <Font size="14.0"/>
            </font>
            <GridPane.margin>
                <Insets left="10.0" right="10.0"/>
            </GridPane.margin>
        </Label>
        <TextField fx:id="conferencePages" prefHeight="25.0" prefWidth="140.0" promptText="Введите страницы"
                   GridPane.columnIndex="1" GridPane.rowIndex="6">
            <GridPane.margin>
                <Insets left="10.0" right="10.0"/>
            </GridPane.margin>
        </TextField>
        <Label alignment="CENTER" prefHeight="17.0" prefWidth="614.0" text="Заполните информацию о конференции"
               textAlignment="CENTER" GridPane.columnSpan="2">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
    </children>
</GridPane>
