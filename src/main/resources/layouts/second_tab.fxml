<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<ScrollPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.121"
            fx:controller="ru.miet.orgact.controllers.SecondTabController">
    <content>
        <AnchorPane prefHeight="314.0" prefWidth="411.0">
            <children>
                <GridPane fx:id="grid" layoutY="2.0" prefHeight="316.0" prefWidth="413.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <CheckBox mnemonicParsing="false" onAction="#changeEnable" text="Web of Science"/>
                        <Spinner disable="true" editable="true" GridPane.columnIndex="1"/>
                        <CheckBox mnemonicParsing="false" onAction="#changeEnable" text="Scopus" GridPane.rowIndex="1"/>
                        <Spinner disable="true" editable="true" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                        <CheckBox mnemonicParsing="false" onAction="#changeEnable" text="e-Library"
                                  GridPane.rowIndex="2"/>
                        <Spinner disable="true" editable="true" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                        <CheckBox mnemonicParsing="false" onAction="#changeEnable" text="Dimensions"
                                  GridPane.rowIndex="3"/>
                        <Spinner disable="true" editable="true" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                    </children>
                </GridPane>
            </children>
        </AnchorPane>
    </content>
</ScrollPane>
